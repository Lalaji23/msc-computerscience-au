#include <stdio.h>
#include<conio.h>
void main ()
{
clrscr();

char inchar ='A';
switch(inchar);
{
case 'A':printf (" choice A\n");
case 'B':
case 'C':printf (" choice B\n");
case 'D':
case 'E':
default :printf ("no choice ");
}
getch();
}     


/output : choice A
         choice B
         no choice/ 
